      },
{% endif %}
{% if domino_leap_proxy_url is defined %}
      {
        "label": "Leap",
        "icon": "Leap",
        "URL": "https://{{ domino_leap_proxy_url }}"
      },
{% endif %}
{% if domino_traveler_proxy_url is defined %}
      {
        "label": "Traveler",
        "icon": "Traveler",
